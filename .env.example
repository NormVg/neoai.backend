# Google AI API Keys (at least 1 required, up to 5 for rotation)
GOOGLE_GENERATIVE_AI_API_KEY=your_primary_key_here
GOOGLE_GENERATIVE_AI_API_KEY_2=
GOOGLE_GENERATIVE_AI_API_KEY_3=
GOOGLE_GENERATIVE_AI_API_KEY_4=
GOOGLE_GENERATIVE_AI_API_KEY_5=

# Supabase PostgreSQL Connection (use pooler/pgbouncer for serverless)
DATABASE_URL=postgresql://postgres.[project-id]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres?pgbouncer=true

# Admin Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_admin_password_here

# JWT Secret (generate a random string)
JWT_SECRET=your_jwt_secret_here

# Dodo Payments (get keys from https://dodopayments.com)
NUXT_PRIVATE_BEARER_TOKEN=your_dodo_api_key
NUXT_PRIVATE_WEBHOOK_KEY=your_dodo_webhook_secret
NUXT_PRIVATE_ENVIRONMENT=test_mode
NUXT_PRIVATE_RETURNURL=https://yoursite.com/pricing?success=1
NUXT_PRIVATE_PRODUCT_2WEEKS=pdt_xxx
NUXT_PRIVATE_PRODUCT_LIFETIME=pdt_xxx
NUXT_PUBLIC_PRODUCT_2WEEKS=pdt_xxx
NUXT_PUBLIC_PRODUCT_LIFETIME=pdt_xxx
NUXT_PUBLIC_PAYMENT_RETURN_URL=https://yoursite.com/pricing?success=1
